@using Zathura.UI.Models
@{
    var programs = ViewBag.Programs as List<Program>;
}

<div class="col-xs-12 col-md-8 col-content">
    <div class="match-list">
        @if (programs != null && programs.Any())
        {
            foreach (var item in programs)
            {
                <div class="match-list-item">
                    <div class="match-list-item-time">@item.Time</div>
                    <div class="match-list-item-info">
                        <div class="match-list-item-name">@item.Name</div>
                        <div class="match-list-item-channel">@item.Channel</div>
                    </div>
                </div>
            }

        }
    </div>
</div>
